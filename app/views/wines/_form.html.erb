<%= form_for @wine do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :region %>
    <%= f.text_field :region %>

    <%= f.label :varietal %>
    <%= f.text_field :varietal %>

    <%= f.label :image_URL %>
    <%= f.url_field :image_URL %>

    <%= f.label :dish_id %>
    <%= f.collection_check_boxes :dish_ids, Dish.all, :id, :name %>

    <%= f.submit %>
<% end %>

